package model.estrutura;

public class DataList {
	Data head;
	Data last;
	
	public DataList() {
		this.head = null;
		this.last = null;
	}
	
	// escrever os dados na fifo
	public void writeData(String data) {
		Data newData = new Data(data);
		
		// primeiro dado
		if (this.head == null) {
			this.head = newData;
			this.last = newData;
		} 
		// outros dados
		else {
			Data lastData = this.last;
			lastData.setNextData(newData);
			this.last = newData;
		}
		
	}
	
	// ler e excluir os dados da FIFO
	public String readData() {
		if (this.head == null) {
			return ;
		}
		
		Data dataToRead = this.head;
		
		String readableData = dataToRead.getData();
		
		this.head = this.head.getNextData();
		
		dataToRead.setNextData(null);
		dataToRead.setData(null);
		
		return readableData;
	}

}